# Configuration Supabase
VITE_SUPABASE_URL=https://votre-projet.supabase.co
VITE_SUPABASE_PUBLISHABLE_KEY=votre_cle_publique_supabase
VITE_SUPABASE_ANON_KEY=votre_cle_anon_supabase

# Configuration Mapbox
VITE_MAPBOX_PUBLIC_TOKEN=votre_token_mapbox

# Configuration S3 (si utilisé)
VITE_S3_ACCESS_KEY=votre_cle_s3_access
VITE_S3_SECRET_KEY=votre_cle_s3_secret
VITE_S3_REGION=votre_region_s3

# Configuration développement
VITE_NODE_ENV=development
VITE_DEBUG=false

# Analytics et tracking (optionnel)
VITE_VERCEL_ANALYTICS_ID=
VITE_SENTRY_DSN=

